<h1>Log in</h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :firstName %><br />
    <%= text_field_tag :firstName, params[:firstName] %>
  </p>
  <p>
    <%= label_tag :lastName %><br />
    <%= text_field_tag :lastName, params[:lastName] %>
  </p>
  <p class="btn"><%= submit_tag "Log in" %></p>
<% end %>
